<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.serverutvlab.database.DBModels.PostEntity">
        <table name="PostEntity" schema="CommunityDB" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id"/>
            </id>
            <basic name="subject">
                <column name="subject"/>
            </basic>
            <basic name="messageBody">
                <column name="messageBody"/>
            </basic>
            <basic name="timestamp">
                <column name="timestamp"/>
            </basic>
            <many-to-one name="getPostedTo" target-entity="com.serverutvlab.database.DBModels.ProfilEntity"/>
        </attributes>
    </entity>
</entity-mappings>
